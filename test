void test2()
{
	char abcdefghijklmnopqrstuvwxyz_____;
	int n;
	float f;
	int is[10];
	char chs[10];
	float fs[10];
	abcdefghijklmnopqrstuvwxyz_____ = '1';
	n = -0;
	is[0]=1234567890;
	is[1]=+1;
	f = -111.0;
	f = ++0;
	fs[0] = f+1*(0+0+f+1/1)-9;
	fs[1] = --0.;
	printf(fs[0]);
}
void test3(char ch)
{
	switch(ch)
	{
		case 'a': printf("aaa");
	}
	;;;
}
char testWhile()
{
	int i;
	i=0;
	while(i<10)
	{
		i = i+1;
	}
	printf(i);
	return ('a');
}
void output(int n)
{
	if(n>0)
	{
		printf(" ",n);
		output(n-1);
	}
	else
		return ;
}
void test4()
{
	int a,b,c;
	scanf(a);
	printf(a);
	scanf(b,c);
	printf(" ",b);
	printf(" ",c);
	scanf(a,b,c);
	printf(" ",a);
	printf(" ",b);
	printf(" ",c);
}
